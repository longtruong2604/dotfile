[credential]
	helper = store
[user]
	name = long.tt
	email = long.tt@atomsolution.com.vn

[alias]
    co = checkout
    br = branch
    st = status
    cm = commit -m
    tree = log --oneline --graph --all --decorate
	amend = commit --amend --no-edit
	aa = add .
	fush = push --force
	pullf = reset --hard HEAD
	undo = reset --soft HEAD
    dev = "!f() { git checkout dev && git pull origin dev; }; f"
	stsh = stash push --keep-index -m  include unstaged only
	staash = stash push --include-untracked -m # include untracked 
	staaash = stash push --all -m # include ignored
	f = fetch
	upstream = push --set-upstream origin
[safe]
	directory = /run/media/longtruong/Data/web-programing/Course/nextjs-super/frontend
